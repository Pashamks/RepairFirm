@{
	var repairChart = ViewData["repairChart"];
	var departmetnCountChart = ViewData["departmetnCountChart"];
}
<!DOCTYPE HTML>
<html>
<head>
<script>
window.onload = function () {
 
var chart = new CanvasJS.Chart("chartContainer", {
	animationEnabled: true,
	theme: "light1",
	title: {
		text: "Популряність типів ремонту"
	},
	axisY: {
		scaleBreaks: {
			autoCalculate: true,
			type: "wavy",
			lineColor: "black"
		},
		gridThickness: 0,
		suffix: "",
		labelWrap: true
	},
	data: [{
		type: "column",
		indexLabel: "{y}",
		dataPoints:  @Html.Raw(@repairChart)
	}]
});
chart.render();
 
var chart2 = new CanvasJS.Chart("chartContainerDepartment", {
	animationEnabled: true,
	theme: "light1",
	title: {
		text: "Кількість укладених контрактів по містах"
	},
	axisY: {
		scaleBreaks: {
			autoCalculate: true,
			type: "wavy",
			lineColor: "black"
		},
		gridThickness: 0,
		suffix: "",
		labelWrap: true
	},
	data: [{
		type: "line",
		indexLabel: "{y}",
		dataPoints:  @Html.Raw(@departmetnCountChart)
	}]
});
chart2.render();
}
</script>
</head>
<body>
<div id="chartContainer" style="height: 370px; width: 100%;"></div>
<div id="chartContainerDepartment" style="height: 370px; width: 100%;"></div>
<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>         