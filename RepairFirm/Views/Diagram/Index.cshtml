@{
    var repairChart = ViewData["repairChart"];
    var departmetnCountChart = ViewData["departmetnCountChart"];
}
<!DOCTYPE HTML>
<html>
<head>
<script>
window.onload = function () {
 
var chart = new CanvasJS.Chart("chartContainer", {
	animationEnabled: true,
	theme: "light1",
	title: {
		text: "Популярність типів ремонту"
	},
	axisY: {
		scaleBreaks: {
			autoCalculate: true,
			type: "wavy",
			lineColor: "black"
		},
		gridThickness: 0,
		suffix: "",
		labelWrap: true
	},
	data: [{
		type: "column",
		indexLabel: "{y}",
		dataPoints:  @Html.Raw(@repairChart)
	}]
});
chart.render();
 
var chart2 = new CanvasJS.Chart("chartContainerDepartment", {
	animationEnabled: true,
	theme: "light1",
	title: {
		text: "Кількість укладених контрактів по містах"
	},
	axisY: {
		scaleBreaks: {
			autoCalculate: true,
			type: "wavy",
			lineColor: "black"
		},
		gridThickness: 0,
		suffix: "",
		labelWrap: true
	},
	data: [{
		type: "line",
		indexLabel: "{y}",
		dataPoints:  @Html.Raw(@departmetnCountChart)
	}]
});
chart2.render();

var chart3 = new CanvasJS.Chart("chartContainertest", {
	animationEnabled: true,
	title: {
		text: "Вартість контрактів у найпопулярніших відділенях"
	},
	toolTip: {
		shared: true
	},
    axisX: {
        suffix: " контракт"
    },
	data: [{
		type: "line",
		name: '@Html.Raw(ViewData["city1"])',
		showInLegend: true,
		dataPoints: @Html.Raw(ViewBag.DataPoints1)
	}, {
		type: "line",
		name: '@Html.Raw(ViewData["city2"])',
		showInLegend: true,
		dataPoints: @Html.Raw(ViewBag.DataPoints2)
	}, {
		type: "line",
		name: '@Html.Raw(ViewData["city3"])',
		showInLegend: true,
		dataPoints: @Html.Raw(ViewBag.DataPoints3)
	}]
});
chart3.render();
 
}
</script>
